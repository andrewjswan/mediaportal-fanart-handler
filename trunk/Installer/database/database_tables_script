BEGIN TRANSACTION;
CREATE TABLE [Image] ([Id] TEXT, [Category] TEXT, [Provider] TEXT, [Key1] TEXT, [Key2] TEXT, [FullPath] TEXT, [SourcePath] TEXT, [AvailableRandom] TEXT, [Enabled] TEXT, [DummyItem] TEXT, [MBID] TEXT, [Time_Stamp] TEXT, CONSTRAINT [i_IdProviderKey1] PRIMARY KEY ([Id], [Provider], [Key1]) ON CONFLICT REPLACE);
CREATE INDEX [i_Category] ON [Image] ([Category]);
CREATE INDEX [i_CategoryTimeStamp] ON [Image] ([Category], [Time_Stamp]);
CREATE INDEX [i_EnabledAvailableRandomCategory] ON [Image] ([Enabled], [AvailableRandom], [Category]);
CREATE INDEX [i_Key1CategoryDummyItem] ON [Image] ([Key1], [Category], [DummyItem]);
CREATE INDEX [i_Key1Key2CategoryDummyItem] ON [Image] ([Key1], [Key2], [Category], [DummyItem]);
CREATE INDEX [i_Key1Enabled] ON [Image] ([Key1], [Enabled]);
CREATE INDEX [i_Key1Key2Enabled] ON [Image] ([Key1], [Key2], [Enabled]);
CREATE INDEX [i_Key1EnabledCategory] ON [Image] ([Key1], [Enabled], [Category]);
CREATE INDEX [i_Key1Key2EnabledCategory] ON [Image] ([Key1], [Key2], [Enabled], [Category]);
CREATE INDEX [i_Key1Category] ON [Image] ([Key1], [Category]);
CREATE INDEX [i_Key1Key2Category] ON [Image] ([Key1], [Key2], [Category]);
CREATE INDEX [i_MBID] ON [Image] ([MBID] COLLATE NOCASE);
CREATE INDEX [i_Key1MBID] ON [Image] ([Key1], [MBID]);
CREATE INDEX [i_Key1Key2MBID] ON [Image] ([Key1], [Key2], [MBID]);
CREATE TABLE [Version] (Id INTEGER PRIMARY KEY, Version TEXT, Time_Stamp TEXT);
COMMIT;
